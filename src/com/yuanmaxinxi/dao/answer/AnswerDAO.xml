<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuanmaxinxi.dao.answer.AnswerDAO">
    <select id="selectAllByMbtiId" resultType="MBTIAnswer">
        select * from t_mbtianswer where mbtiId = #{mbtiId}
    </select>
    <select id="selectOneById" parameterType="long" resultType="MBTIAnswer">
        select * from t_mbtianswer where id = #{id}
    </select>
</mapper>